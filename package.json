{
  "name": "contentful-roku",
  "version": "0.1.0",
  "description": "BrightScript Contentful SDK",
  "keywords": [
    "brightscript",
    "kopytko",
    "kopytko-module",
    "contentful",
    "roku"
  ],
  "author": "Błażej Chełkowski <blazej.chelkowski@outlook.com>",
  "files": [
    "src",
    "!**/_tests"
  ],
  "scripts": {
